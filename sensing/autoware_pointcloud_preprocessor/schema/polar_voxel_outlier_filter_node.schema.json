{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parameters for Polar Voxel Outlier Filter Node",
  "type": "object",
  "definitions": {
    "polar_voxel_outlier_filter": {
      "type": "object",
      "properties": {
        "radius_resolution": {
          "type": "number",
          "description": "the resolution in radial direction [m]",
          "default": "0.5",
          "minimum": 0
        },
        "azimuth_resolution": {
          "type": "number",
          "description": "the resolution in azimuth direction [rad]",
          "default": "0.0174",
          "minimum": 0
        },
        "elevation_resolution": {
          "type": "number",
          "description": "the resolution in elevation direction [rad]",
          "default": "0.0174",
          "minimum": 0
        },
        "voxel_points_threshold": {
          "type": "integer",
          "description": "the minimum number of points required per voxel",
          "default": "2",
          "minimum": 1
        },
        "min_radius": {
          "type": "number",
          "description": "the minimum radius to consider [m]",
          "default": "0.5",
          "minimum": 0
        },
        "max_radius": {
          "type": "number",
          "description": "the maximum radius to consider [m]",
          "default": "100.0",
          "minimum": 0
        },
        "use_return_type_classification": {
          "type": "boolean",
          "description": "whether to use return type classification (if false, all returns are processed)",
          "default": "true"
        },
        "filter_secondary_returns": {
          "type": "boolean",
          "description": "whether to filter secondary returns (if true, only primary returns are kept)",
          "default": "false"
        },
        "secondary_noise_threshold": {
          "type": "number",
          "description": "threshold for classifying primary vs secondary returns",
          "default": "1.0",
          "minimum": 0
        },
        "primary_return_types": {
          "type": "array",
          "description": "list of return type values considered as primary returns (all others are secondary)",
          "items": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255
          },
          "default": "[1]"
        },
        "filter_ratio_error_threshold": {
          "type": "number",
          "description": "the filter ratio threshold for error diagnostics (ratio of output/input points)",
          "default": "0.5",
          "minimum": 0,
          "maximum": 1
        },
        "filter_ratio_warn_threshold": {
          "type": "number",
          "description": "the filter ratio threshold for warning diagnostics (ratio of output/input points)",
          "default": "0.7",
          "minimum": 0,
          "maximum": 1
        },
        "visibility_error_threshold": {
          "type": "number",
          "description": "the visibility threshold for error diagnostics",
          "default": "0.5",
          "minimum": 0,
          "maximum": 1
        },
        "visibility_warn_threshold": {
          "type": "number",
          "description": "the visibility threshold for warning diagnostics",
          "default": "0.7",
          "minimum": 0,
          "maximum": 1
        }
      },
      "required": [
        "radius_resolution",
        "azimuth_resolution",
        "elevation_resolution",
        "voxel_points_threshold",
        "min_radius",
        "max_radius",
        "use_return_type_classification",
        "filter_secondary_returns",
        "secondary_noise_threshold",
        "primary_return_types",
        "filter_ratio_error_threshold",
        "filter_ratio_warn_threshold",
        "visibility_error_threshold",
        "visibility_warn_threshold"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "/**": {
      "type": "object",
      "properties": {
        "ros__parameters": {
          "$ref": "#/definitions/polar_voxel_outlier_filter"
        }
      },
      "required": ["ros__parameters"],
      "additionalProperties": false
    }
  },
  "required": ["/**"],
  "additionalProperties": false
}
