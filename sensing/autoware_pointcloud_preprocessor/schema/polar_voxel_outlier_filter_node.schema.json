{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parameters for Polar Voxel Outlier Filter Node",
  "type": "object",
  "definitions": {
    "polar_voxel_outlier_filter": {
      "type": "object",
      "properties": {
        "radius_resolution": {
          "type": "number",
          "description": "the resolution in radial direction [m]",
          "default": "0.5",
          "minimum": 0
        },
        "azimuth_resolution": {
          "type": "number",
          "description": "the resolution in azimuth direction [rad]",
          "default": "0.0174",
          "minimum": 0
        },
        "elevation_resolution": {
          "type": "number",
          "description": "the resolution in elevation direction [rad]",
          "default": "0.0174",
          "minimum": 0
        },
        "voxel_points_threshold": {
          "type": "integer",
          "description": "the minimum number of points required per voxel",
          "default": "2",
          "minimum": 1
        },
        "min_radius": {
          "type": "number",
          "description": "the minimum radius to consider [m]",
          "default": "0.5",
          "minimum": 0
        },
        "max_radius": {
          "type": "number",
          "description": "the maximum radius to consider [m]",
          "default": "100.0",
          "minimum": 0
        }
      },
      "required": ["radius_resolution", "azimuth_resolution", "elevation_resolution", "voxel_points_threshold", "min_radius", "max_radius"],
      "additionalProperties": false
    }
  },
  "properties": {
    "/**": {
      "type": "object",
      "properties": {
        "ros__parameters": {
          "$ref": "#/definitions/polar_voxel_outlier_filter"
        }
      },
      "required": ["ros__parameters"],
      "additionalProperties": false
    }
  },
  "required": ["/**"],
  "additionalProperties": false
} 