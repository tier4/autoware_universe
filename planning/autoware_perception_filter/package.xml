<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="package_format3.xsd"?>
<package format="3">
  <name>autoware_perception_filter</name>
  <version>0.1.0</version>
  <description>Perception filter package that filters perception data based on external approval</description>
  <maintainer email="kyoichi.sugahara@tier4.jp">Kyoichi Sugahara</maintainer>
  <maintainer email="satoshi.ota@tier4.jp">Satoshi Ota</maintainer>
