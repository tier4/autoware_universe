<launch>
  <arg name="input"/>
  <arg name="output"/>
  <arg name="node_name" default="detected_object_feature_remover"/>

  <node_container pkg="agnocastlib" exec="agnocast_component_container" name="$(var node_name)_container" namespace="" output="screen">
    <env name="LD_PRELOAD" value="/opt/ros/humble/lib/libagnocast_heaphook.so:$(env LD_PRELOAD '')"/>
    <composable_node pkg="autoware_detected_object_feature_remover" plugin="autoware::detected_object_feature_remover::DetectedObjectFeatureRemover" name="$(var node_name)" namespace="">
      <remap from="~/input" to="$(var input)"/>
      <remap from="~/output" to="$(var output)"/>
    </composable_node>
  </node_container>
</launch>
