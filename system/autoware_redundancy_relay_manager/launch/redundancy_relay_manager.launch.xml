<launch>
  <node pkg="autoware_redundancy_relay_manager" exec="autoware_redundancy_relay_manager_node" name="redundancy_relay_manager" output="screen"/>
  <arg name="config_file" default="$(find autoware_redundancy_relay_manager)/config/redundancy_relay_manager.yaml"/>
  <arg name="input_operation_mode_state" default="/system/operation_mode/state"/>
  <arg name="input_election_status" default="/system/election/status"/>

  <node pkg="autoware_redundancy_relay_manager" exec="autoware_redundancy_relay_manager_node" name="redundancy_relay_manager" output="screen">
    <param name="config_file" from="$(var config_file)"/>
    <remap from="~/input/operation_mode/state" to="$(var input_operation_mode_state)"/>
    <remap from="~/input/election/status" to="$(var input_election_status)"/>
  </node>
</launch>
